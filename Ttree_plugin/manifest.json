{
  "name": "T-tree",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "manage your tab",
  "homepage_url": "http://ttree.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "T-tree"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}
